{
  "version": 2,
  "name": "express-product",
  "builds": [
    { "src": "./index.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "./index.js" },
    {
      "src": "/",
      "dest": "public/Resume.pdf"
    }
  ]
}
